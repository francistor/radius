                <designator group="TacacsPlusAcctServer" instance="*" variable="Error" />
            </variable>
            <variable name="Packets Dropped" expression="$1" color="Olive">
                <designator group="TacacsPlusAcctServer" instance="*" variable="PacketsDropped" />
            </variable>
        </view>
    </monitor>
    <monitor name="USS Replication" interval="10s" treePath="USS">
        <view title="USS Replication" displayPeriod="30Minutes">
            <variable name="Discovery Failures" expression="$1" color="Blue">
                <designator group="USS" instance="*" variable="Replication-Discovery-Failures" />
            </variable>
            <variable name="Merged Entries" expression="$1" color="Green">
                <designator group="USS" instance="*" variable="Replication-Entries-Merged" />
            </variable>
            <variable name="Entries Updated" expression="$1" color="Red">
                <designator group="USS" instance="*" variable="Replication-Entries-Updated" />
            </variable>
            <variable name="Reconciliations" expression="$1" color="Olive">
                <designator group="USS" instance="*" variable="Replication-Reconciliations" />
            </variable>
        </view>
    </monitor>
    <monitor name="USS Requests" interval="10s" treePath="USS">
        <view title="USS Requests" displayPeriod="30Minutes">
            <variable name="Total Requests" expression="$1" color="Blue">
                <designator group="USS" instance="*" variable="Total-Requests" />
            </variable>
            <variable name="Invalid Requests" expression="$1" color="Green">
                <designator group="USS" instance="*" variable="Invalid-Requests" />
            </variable>
            <variable name="Limit Reject" expression="$1" color="Red">
                <designator group="USS" instance="*" variable="Limit-Rejects" />
            </variable>
            <variable name="Not Handled" expression="$1" color="Yellow">
                <designator group="USS" instance="*" variable="Not-Handled" />
            </variable>
            <variable name="Exceptions" expression="$1" color="Olive">
                <designator group="USS" instance="*" variable="Exceptions" />
            </variable>
            <variable name="Map Evaluation Errors" expression="$1" color="Magenta">
                <designator group="USS" instance="*" variable="Map-Evaluation-Errors" />
            </variable>
        </view>
    </monitor>
    <monitor name="USS Session State" interval="10s" treePath="uss">
        <view title="USS Session States" displayPeriod="30Seconds">
            <variable name="Active State" expression="$1" color="Blue">
                <designator group="USS" instance="*" variable="State-Active" />
            </variable>
            <variable name="Inactive State" expression="$1" color="Green">
                <designator group="USS" instance="*" variable="State-Inactive" />
            </variable>
            <variable name="Waiting For Start" expression="$1" color="Yellow">
                <designator group="USS" instance="*" variable="State-Waiting-For-Start" />
            </variable>
        </view>
    </monitor>
    <monitor name="Memory Usage" interval="10s">
        <view title="Memory Usage" subtitle="Displays the Memory Usage for the Java Virtual Machine and Operating System" displayPeriod="30Minutes">
            <variable name="Java Initial" expression="$1" color="Yellow" forcedScale="mega" visible="false">
                <designator group="JavaMemory" instance="*" variable="HeapMemoryInit" />
            </variable>
            <variable name="Java Used" expression="$1" color="Green" forcedScale="mega">
                <designator group="JavaMemory" instance="*" variable="HeapMemoryUsed" />
            </variable>
            <variable name="Java Committed" expression="$1" color="Red" forcedScale="mega">
                <designator group="JavaMemory" instance="*" variable="HeapMemoryCommitted" />
            </variable>
            <variable name="Java Max" expression="$1" color="Blue" forcedScale="mega" visible="false">
                <designator group="JavaMemory" instance="*" variable="HeapMemoryMax" />
            </variable>
        </view>
    </monitor>
    <monitor name="Work Engine" interval="10s">
        <view title="Total Items" subtitle="Total Number of Work Items Processed" viewType="Table" displayPeriod="1Hours">
            <variable name="Total Items" expression="$1" color="Dark Green">
                <designator group="Engine" instance="*" variable="ItemTotalCount" />
            </variable>
            <variable name="Total Item Time" expression="$1" color="Green">
                <designator group="Engine" instance="*" variable="ItemTotalTime" />
            </variable>
            <variable name="Average Item Time" expression="$1/$2" color="Blue">
                <designator group="Engine" instance="*" variable="ItemTotalTime" />
                <designator group="Engine" instance="*" variable="ItemTotalCount" />
            </variable>
            <variable name="Total PolicyFlow Methods" expression="$1" color="Light Blue">
                <designator group="Engine" instance="*" variable="MethodCount" />
            </variable>
            <variable name="Minimum Item Time" expression="$1" color="Magenta">
                <designator group="Engine" instance="*" variable="ItemMinTime" />
            </variable>
            <variable name="Maximum Item Time" expression="$1" color="Orange">
                <designator group="Engine" instance="*" variable="ItemMaxTime" />
            </variable>
        </view>
        <view title="Average Item Time" viewType="Graph" displayPeriod="30Minutes">
            <variable name="Average Item Time" expression="$1/$2" color="Blue">
                <designator group="Engine" instance="*" variable="ItemTotalTime" />
                <designator group="Engine" instance="*" variable="ItemTotalCount" />
            </variable>
        </view>
        <view title="Load" viewType="Graph" displayPeriod="30Minutes">
            <variable name="Total Items" expression="$1" color="Dark Green">
                <designator group="Engine" instance="*" variable="ItemTotalCount" />
            </variable>
            <variable name="CPU Load" expression="$1/$2" color="Blue">
                <designator group="OperatingSystem" instance="*" variable="ProcessCpuTime" />
                <designator group="OperatingSystem" instance="*" variable="AvailableProcessors" />
            </variable>
        </view>
    </monitor>
    <monitor name="Log Queue" interval="10s" treePath="Queues" includeZero="false">
        <view title="Log Queue" subtitle="Current and HighWater values for logs to write to log_channels" displayPeriod="30Minutes">
            <variable name="Log" expression="$1/$2" color="Magenta">
                <designator group="Queue" instance="log" variable="QueueDepthCount" />
                <designator group="Queue" instance="log" variable="QueueWriteCount" />
            </variable>
            <variable name="Log Highwater" expression="$1" color="Dark Magenta" lineStyle="Dashed">
                <designator group="Queue" instance="log" variable="QueueHighwater" />
            </variable>
        </view>
    </monitor>
    <monitor name="Work Queues" interval="10s" treePath="Queues" includeZero="false">
        <view title="Work Queues" subtitle="Current and HighWater values for queues of incoming requests" viewType="Table" displayPeriod="30Minutes">
            <variable name="Resume" expression="$1/$2" color="Red">
                <designator group="Queue" instance="work-resume" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-resume" variable="QueueWriteCount" />
            </variable>
            <variable name="Resume Highwater" expression="$1" color="Crimson" lineStyle="Dashed">
                <designator group="Queue" instance="work-resume" variable="QueueHighwater" />
            </variable>
            <variable name="State" expression="$1/$2" color="Ocean Blue">
                <designator group="Queue" instance="work-state" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-state" variable="QueueWriteCount" />
            </variable>
            <variable name="State Highwater" expression="$1" color="Blue" lineStyle="Dashed">
                <designator group="Queue" instance="work-state" variable="QueueHighwater" />
            </variable>
            <variable name="Normal" expression="$1/$2" color="Green">
                <designator group="Queue" instance="work-normal" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-normal" variable="QueueWriteCount" />
            </variable>
            <variable name="Normal Highwater" expression="$1" color="Dark Green" lineStyle="Dashed">
                <designator group="Queue" instance="work-normal" variable="QueueHighwater" />
            </variable>
            <variable name="Background" expression="$1/$2" color="Tan">
                <designator group="Queue" instance="work-background" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-background" variable="QueueWriteCount" />
            </variable>
            <variable name="Background Highwater" expression="$1" color="Brown" lineStyle="Dashed">
                <designator group="Queue" instance="work-background" variable="QueueHighwater" />
            </variable>
        </view>
        <view title="Work Queues - Current size" subtitle="Current values for queues of incoming requests" viewType="Graph" displayPeriod="30Minutes">
            <variable name="Resume" expression="$1/$2" color="Red">
                <designator group="Queue" instance="work-resume" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-resume" variable="QueueWriteCount" />
            </variable>
            <variable name="State" expression="$1/$2" color="Ocean Blue">
                <designator group="Queue" instance="work-state" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-state" variable="QueueWriteCount" />
            </variable>
            <variable name="Normal" expression="$1/$2" color="Green">
                <designator group="Queue" instance="work-normal" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-normal" variable="QueueWriteCount" />
            </variable>
            <variable name="Background" expression="$1/$2" color="Tan">
                <designator group="Queue" instance="work-background" variable="QueueDepthCount" />
                <designator group="Queue" instance="work-background" variable="QueueWriteCount" />
            </variable>
        </view>
        <view title="Work Queues - Highwater size" subtitle="HighWater values for queues of incoming requests" viewType="Graph" displayPeriod="30Minutes">
            <variable name="Resume Highwater" expression="$1" color="Crimson" lineStyle="Dashed">
                <designator group="Queue" instance="work-resume" variable="QueueHighwater" />
            </variable>
            <variable name="State Highwater" expression="$1" color="Blue" lineStyle="Dashed">
                <designator group="Queue" instance="work-state" variable="QueueHighwater" />
            </variable>
            <variable name="Normal Highwater" expression="$1" color="Dark Green" lineStyle="Dashed">
                <designator group="Queue" instance="work-normal" variable="QueueHighwater" />
            </variable>
            <variable name="Background Highwater" expression="$1" color="Brown" lineStyle="Dashed">
                <designator group="Queue" instance="work-background" variable="QueueHighwater" />
            </variable>
        </view>
    </monitor>
</monitors>
